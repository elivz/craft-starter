# The environment Craft is currently running in ('dev', 'staging', 'live', etc.)
ENVIRONMENT=dev

# Base URL and path of the site
BASE_URL=https://website.com
BASE_PATH=/var/www/public_html

# The secure key Craft will use for hashing and encrypting data
SECURITY_KEY=""

# Local database connection
DB_HOST=localhost
DB_NAME=REPLACE_ME
DB_USER=REPLACE_ME
DB_PASS=REPLACE_ME
DB_PORT=3306

# Redis connection
REDIS_HOST=localhost
REDIS_PORT=6379
