{
  "name": "elivz/craft-starter",
  "description": "Craft CMS project template",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "MIT",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "require": {
    "aelvan/imager": "^2.1",
    "craftcms/cms": "^3.1.0-beta1",
    "craftcms/redactor": "^2.0",
    "nystudio107/craft-minify": "1.2.9",
    "nystudio107/craft-seomatic": "^3.1",
    "nystudio107/craft-twigpack": "^1.1",
    "nystudio107/craft-typogrify": "^1.1",
    "roave/security-advisories": "dev-master",
    "verbb/expanded-singles": "^1.0",
    "vlucas/phpdotenv": "^2.4",
    "yiisoft/yii2-redis": "^2.0"
  },
  "prefer-stable": true,
  "minimum-stability": "beta",
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "vendor-dir": "dist/vendor",
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php -r \"unlink('LICENSE.md');\"",
      "@composer dump-autoload -o",
      "@php craft setup/welcome"
    ],
    "post-install-cmd": [
      "dist/craft migrate/all"
    ],
    "post-update-cmd": [
      "dist/craft migrate/all"
    ]
  }
}
